<h3>Boards Index</h3>

<div class="btn-group btn-block">
  
  <% boards.each(function(board){ %>
    <a href="#/boards/<%= board.escape("id") %>"
      class="boardTile">
      <div class="btn btn-primary col-xs-3">
          <%= board.escape("title") %>
      </div>
    </a>
  <% }); %>


   

</div>
